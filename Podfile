platform :ios, '13.0'
use_frameworks!

target 'tarot_cards' do

  # Layout DSL
  pod 'SnapKit'

  # JSON / Plist helper (optional if you use Codable)
  pod 'SwiftyJSON'
  
  pod 'Alamofire'
  
  pod 'SwifterSwift'

  pod 'Dollar'
end

# Usage:
# 1. 在项目目录下运行 `pod install`
# 2. 之后打开生成的 `.xcworkspace` 文件来工作
